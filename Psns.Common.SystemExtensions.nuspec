<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Psns.Common.SystemExtensions</id>
    <authors>PSNS</authors>
    <owners>109.14</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Some helpful extension methods</description>
    <projectUrl>https://github.com/PSNS-IMF/system-extensions</projectUrl>
    <licenseUrl>https://github.com/PSNS-IMF/system-extensions/blob/master/LICENSE.md</licenseUrl>
    <releaseNotes>
      ## 2.0.0.2
      ### Bug Fixes
      * Fixed bug with Chunk extension skipping items

      ## 2.0.0.1
      ### Features
      * Allow throttling of Error logging

      ## 2.0.0.0
      ### Features
      * Switched to NetStandard in order to support all platforms

      ## 1.0.0.5
      ### Features
      * Fixed issue with TryUse disposing of target before Task finishes.

      ## 1.0.0.4
      ### Features
      * Adding more overloads and helpers.

      ## 1.0.0.3
      ### Features
      * Adding in some functional helpers and Monads.

      ## 1.0.0.2
      ### Features
      * Fixed issue with ConnectAsync not awaiting OpenAsync

      ## 1.0.0.1
      ### Features
      * Adding ProcessState and removing conflicting functions

      ## 1.0.0.0
      ### Features
      * Initial release
    </releaseNotes>
    <version>$version$</version>
    <tags>PSNS</tags>
    <dependencies>
      <group targetFramework=".NETStandard1.6">
        <dependency id="NETStandard.Library" version="2.0.2" exclude="Build,Analyzers" />
        <dependency id="System.Data.SqlClient" version="4.4.3" exclude="Build,Analyzers" />
        <dependency id="System.Diagnostics.Process" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Diagnostics.TraceSource" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Threading.Thread" version="4.3.0" exclude="Build,Analyzers" />
        <dependency id="System.Threading.ThreadPool" version="4.3.0" exclude="Build,Analyzers" />
      </group>
      <group>
        <dependency id="Psns.Common.Functional" version="2018.5.14.1" exclude="Build,Analyzers" />
        <dependency id="Psns.Common.Analysis" version="2018.5.9.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="Psns.Common.SystemExtensions\bin\$configuration$\**\Psns.Common.SystemExtensions.*" target="lib\" exclude="**\*.dll.config;**\*Tests*;**\*Test*;**\*.deps.json" />
  </files>
</package>